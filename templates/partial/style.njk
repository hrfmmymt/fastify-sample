/*
 *common and initial settings
 */
a,
article,
blockquote,
body,
div,
footer,
h1,
h2,
h3,
header,
html,
li,
nav,
p,
time,
ul {
  margin: 0;
  padding: 0;
}

:root {
  --text-color: #000;
  --text-color-sub: #555;
  --text-color-anchor: #db2c17;
  --page-background-color: #fff;
  --code-background-color: #f4f4f4;
}

@media (prefers-color-scheme: dark) {
  :root {
    --text-color: #fff;
    --text-color-sub: #f2f1ef;
    --text-color-anchor: #00b5b5;
    --page-background-color: #332f21;
    --code-background-color: #000;
  }
}

html {
  font-size: calc(1em + 0.5vw);
}

body {
  font-family: 'Open Sans', 'Helvetica Neue', sans-serif;
  font-kerning: normal;
  -webkit-font-smoothing: antialiased;
  text-rendering: optimizeLegibility;
  background-color: var(--page-background-color);
  color: var(--text-color);
}

a {
  color: var(--text-color-anchor);
  text-decoration: none;
}

a:hover {
  text-decoration: underline;
}

a:visited {
  color: inherit;
}

ul {
  list-style-type: none;
}

.post-article,
.footer,
.header {
  margin: 0 auto;
  line-height: 2;
}

.header {
  margin: 1.5rem 0 1.2rem;
  text-align: center;
}

.header-title {
  font-size: 1rem;
  letter-spacing: 0.05rem;
}

.header-title .header-title-link,
.footer .footer-link,
.header-title .header-title-link:visited,
.footer .footer-link:visited {
  color: inherit;
}

.header-title .header-title-link {
  display: block;
  margin: 0 auto;
  width: 6rem;
}

.logo {
  fill: var(--text-color);
}

.post-list,
.profile > *,
.post-article {
  max-width: 32rem;
  margin: 0 auto;
  padding: 0 1rem;
}

.footer {
  margin-top: 2.4rem;
  padding: 1rem;
  font-size: small;
  text-align: center;
}

/*
 * Top page
 */
/* profile */
.profile {
  padding: 3rem 0;
}

.profile-img-wrapper {
  float: right;
}

.profile-name {
  margin-bottom: 2rem;
}

.profile-list {
  display: flex;
  flex-wrap: wrap;
  font-size: 0.9rem;
  line-height: 2.4;
}

.profile-list-item {
  margin-right: 1rem;
}

.profile-list-link {
  text-decoration: underline;
}

.profile-list-link:hover {
  text-decoration: none;
}

.profile-img {
  padding: 0;
  border-radius: 50%;
}

/* post list */
.post-list {
  border-top: 1px solid #ddd;
}

.post-item {
  margin: 3rem 0;
}

.post-title {
  font-size: 1.5em;
  font-weight: bold;
  margin-bottom: 0.4rem;
}

.post-meta,
.post-article time,
.post-article .description {
  font-weight: 400;
  color: var(--text-color-sub);
  word-wrap: break-word;
}

.post-link:hover {
  text-decoration: none;
}

.post-link:hover .post-title {
  text-decoration: underline;
}

.post-date,
.post-article time {
  font-size: 0.8rem;
}

/*
 * post page
 */
.post-article h1 {
  font-size: 2rem;
}

.post-article h2 {
  margin-bottom: 1rem;
  border-bottom: 1px solid;
}

.post-article h3 {
  margin-top: 0.5rem;
  margin-bottom: -1rem;
}

.post-article h4 {
  margin: 0 0 -1rem;
}

.post-article p {
  margin: 1rem 0;
  word-wrap: break-word;
}

.post-article a {
  text-decoration: underline;
}

.post-article a:hover {
  text-decoration: none;
}

.post-article li a {
  word-break: break-all;
}

.post-article img,
.post-article video {
  height: auto;
  width: 100%;
}

.post-article code {
  border: 1px solid #ddd;
  background-color: var(--code-background-color);
  border-radius: 2px;
  padding: 0.1em;
  margin: 1.4em 0;
}

.post-article pre > code {
  overflow: auto;
  display: block;
  padding: 0.6em;
  margin: 0;
  border: none;
  background-color: var(--code-background-color);
}

.post-article blockquote {
  padding: 0 1em;
  margin: 1em 0;
  color: var(--text-color-sub);
  border-left: 0.25em solid #dfe2e5;
  word-wrap: break-word;
}

.post-article ul {
  list-style-type: disc;
  margin: 1rem 0;
  padding-left: 2rem;
}

.post-article ul li ul {
  list-style-type: circle;
}

.post-article ol {
  padding-left: 2rem;
}

.post-article .description {
  font-size: 0.9rem;
}

/* highlight.js */
.hljs {
  display: block;
  overflow-x: auto;
  padding: 0.5em;
  background: #f0f0f0;
}

.hljs,
.hljs-subst {
  color: #444;
}

.hljs-comment {
  color: #888;
}

.hljs-keyword,
.hljs-attribute,
.hljs-selector-tag,
.hljs-meta-keyword,
.hljs-doctag,
.hljs-name {
  font-weight: bold;
}

.hljs-type,
.hljs-string,
.hljs-number,
.hljs-selector-id,
.hljs-selector-class,
.hljs-quote,
.hljs-template-tag,
.hljs-deletion {
  color: #800;
}

.hljs-title,
.hljs-section {
  color: #800;
  font-weight: bold;
}

.hljs-regexp,
.hljs-symbol,
.hljs-variable,
.hljs-template-variable,
.hljs-link,
.hljs-selector-attr,
.hljs-selector-pseudo {
  color: #bc6060;
}

.hljs-literal {
  color: #78a960;
}

.hljs-built_in,
.hljs-bullet,
.hljs-code,
.hljs-addition {
  color: #397300;
}

.hljs-meta {
  color: #1f7199;
}

.hljs-meta-string {
  color: #4d99bf;
}

.hljs-emphasis {
  font-style: italic;
}

.hljs-strong {
  font-weight: bold;
}

/*
 * for mobile
 */
@media (max-width: 414px) {
  .profile {
    padding: 0 0 1rem;
  }

  .profile-img-wrapper {
    float: none;
  }

  .profile-img {
    max-width: 80px;
    max-height: 80px;
  }

  .profile h2 {
    margin: 1rem 0;
  }
}

